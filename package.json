{
  "name": "truffle-init-webpack",
  "version": "0.0.2",
  "description": "Frontend example using truffle v3",
  "scripts": {
    "lint": "eslint ./",
    "build": "webpack",
    "dev": "webpack-dev-server",
    "test": "truffle migrate --reset && truffle test",
    "watch": "npm-watch"
  },
  "watch": {
    "test": {
      "patterns": [
        "contracts",
        "test"
      ],
      "quiet": true,
      "delay": 2500
    }
  },
